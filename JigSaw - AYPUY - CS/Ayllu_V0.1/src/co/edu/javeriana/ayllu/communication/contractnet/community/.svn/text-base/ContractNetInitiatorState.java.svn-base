package co.edu.javeriana.ayllu.communication.contractnet.community;

import BESA.Kernel.System.Directory.AgHandlerBESA;
import co.edu.javeriana.ayllu.agents.communityagent.CommunityAgentState;
import java.util.List;

/**
 *
 * @author AylluAdmin
 */
public class ContractNetInitiatorState extends CommunityAgentState{
    private int participantsNumber;
    private int participantsResponded;
    //private List<AgHandler> presentedProposals;
    private List<AgHandlerBESA> acceptedProposals;
    public ContractNetInitiatorState(int participantsNumber) {
        super(null, null, null);
        this.participantsNumber = participantsNumber;
    }

    public List<AgHandlerBESA> getAcceptedProposals() {
        return acceptedProposals;
    }

    public int getParticipantsNumber() {
        return participantsNumber;
    }

    public int getParticipantsResponded() {
        return participantsResponded;
    }

    
    
}
