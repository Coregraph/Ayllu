package co.edu.javeriana.ayllu.agents.communityagent;

import BESA.Kernel.System.Directory.AgHandlerBESA;

/**
 * AgentRole allows a CMA to administer a group of agents with their corresponding role in the cooperative service
 * @author AYLLU
 */
public class AgentRole {
    
    String role;
    AgHandlerBESA communityAgentHandler;
    /**
     * Constructor of the AgentRole, receives a role that is assigned to a handler
     * @param role a string that represents the role of the agent
     * @param communityAgentHandler the handler of the agent that will assume that role
     */
    public AgentRole(String role, AgHandlerBESA communityAgentHandler) {
        this.role = role;
        this.communityAgentHandler = communityAgentHandler;
    }
    /**
     * 
     * @return 
     */
    public AgHandlerBESA getCommunityAgentHandler() {
        return communityAgentHandler;
    }

    public void setCommunityAgentHandler(AgHandlerBESA communityAgentHandler) {
        this.communityAgentHandler = communityAgentHandler;
    }

    public String getRole() {
        return role;
    }

    public void setRole(String role) {
        this.role = role;
    }
    
    
    
}
