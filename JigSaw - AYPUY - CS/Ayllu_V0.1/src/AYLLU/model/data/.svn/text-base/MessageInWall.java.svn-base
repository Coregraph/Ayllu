package AYLLU.model.data;

import java.io.Serializable;

/**
 *
 * @author ANDREA
 */
public class MessageInWall implements Serializable {

    private String cooperativeServiceId;
    private String cooperativeServiceName;
    private long dateMilliseconds;
    private String message;
    private String senderName;

   public MessageInWall(String cooperativeServiceId, String cooperativeServiceName, long dateMilliseconds, String sender, String message) {
        this.cooperativeServiceId = cooperativeServiceId;
        this.cooperativeServiceName = cooperativeServiceName;
        this.dateMilliseconds = dateMilliseconds;
        this.senderName = sender;
        this.message = message;
    }

    public String getCooperativeServiceId() {
        return cooperativeServiceId;
    }

    public void setCooperativeServiceId(String cooperativeServiceId) {
        this.cooperativeServiceId = cooperativeServiceId;
    }

    public String getCooperativeServiceName() {
        return cooperativeServiceName;
    }

    private void setCooperativeServiceName(String cooperativeServiceName) {
        this.cooperativeServiceName = cooperativeServiceName;
    }

    public long getDateMilliseconds() {
        return dateMilliseconds;
    }

    private void setDateMilliseconds(long dateMilliseconds) {
        this.dateMilliseconds = dateMilliseconds;
    }

    public String getMessage() {
        return message;
    }

    private void setMessage(String message) {
        this.message = message;
    }

    public String getSenderName() {
        return senderName;
    }

    public void setSenderName(String senderName) {
        this.senderName = senderName;
    }
}
