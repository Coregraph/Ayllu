package service.Profesor;

import BESA.Kernel.Agent.Event.DataBESA;

/**
 *
 * @author Yolima
 */
public class DatosRetroNota extends DataBESA{
    
    private String nota;
    private String retroalimentacion;  
    private String aliasCma;

    public DatosRetroNota() {
        retroalimentacion = "";
        nota = "";
        aliasCma = "";
    }

    public DatosRetroNota(String nota, String retro, String aliascma) {
        this.nota = nota;
        this.retroalimentacion = retro;
        this.aliasCma = aliascma;
    }

    public String getNota() {
        return nota;
    }

    public void setNota(String nota) {
        this.nota = nota;
    }

    public String getRetroalimentacion() {
        return retroalimentacion;
    }

    public void setRetroalimentacion(String retroalimentacion) {
        this.retroalimentacion = retroalimentacion;
    }

    public String getAliasCma() {
        return aliasCma;
    }

    public void setAliasCma(String aliasCma) {
        this.aliasCma = aliasCma;
    }
   
}
