package servicios.recuperarInformacion.aprendiz;

import BESA.Kernel.System.Directory.AgHandlerBESA;
import co.edu.javeriana.ayllu.agents.communityagent.AgentRole;
import co.edu.javeriana.ayllu.agents.communityagent.CommunityAgentState;
import java.util.List;

/**
 *
 * @author AylluAdmin
 */
public class DatosAprendizRI extends CommunityAgentState{
    
    private String OA;
    private boolean OALeido;
    private List<String> estudiantesConOALeido;

    public DatosAprendizRI(List<AgentRole> sameVolatileGroupHandlers, AgHandlerBESA sessionManagerHandler, AgHandlerBESA communityManagerHandler) {
       super(sameVolatileGroupHandlers, sessionManagerHandler, communityManagerHandler);
               
    }

    /**
     * @return the OA
     */
    public String getOA() {
        return OA;
    }

    /**
     * @param OA the OA to set
     */
    public void setOA(String OA) {
        this.OA = OA;
    }

    

    /**
     * @return the OALeido
     */
    public boolean isOALeido() {
        return OALeido;
    }

    /**
     * @param OALeido the OALeido to set
     */
    public void setOALeido(boolean OALeido) {
        this.OALeido = OALeido;
    }

    /**
     * @return the estudiantesConOALeido
     */
    public List<String> getEstudiantesConOALeido() {
        return estudiantesConOALeido;
    }

    /**
     * @param estudiantesConOALeido the estudiantesConOALeido to set
     */
    public void setEstudiantesConOALeido(List<String> estudiantesConOALeido) {
        this.estudiantesConOALeido = estudiantesConOALeido;
    }



}
