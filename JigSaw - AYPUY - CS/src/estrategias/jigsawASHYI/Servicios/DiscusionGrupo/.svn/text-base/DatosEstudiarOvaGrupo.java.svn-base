package estrategias.jigsawASHYI.Servicios.DiscusionGrupo;

import BESA.Kernel.Agent.Event.DataBESA;

/**
 *
 * @author Yolima
 */
public class DatosEstudiarOvaGrupo extends DataBESA{
    
    private int numEstudiantes;
    private int numEstudiantesEstudio;

    public DatosEstudiarOvaGrupo(int numEstudiantes) {
        this.numEstudiantes = numEstudiantes;
        numEstudiantesEstudio = 0;
    }

    public int getNumEstudiantes() {
        return numEstudiantes;
    }

    public void setNumEstudiantes(int numEstudiantes) {
        this.numEstudiantes = numEstudiantes;
    }

    public int getNumEstudiantesEstudio() {
        return numEstudiantesEstudio;
    }

    public void setNumEstudiantesEstudio(int numEstudiantesEstudio) {
        this.numEstudiantesEstudio = numEstudiantesEstudio;
    }

  

}
